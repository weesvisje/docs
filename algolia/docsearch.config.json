{
    "index_name": "docs",
    "renderJavaScript": true,
    "start_urls": [
      {
        "url": "https://docs.ditto.live/"
      }
    ],
    "stop_urls": [
      "changelog",
      "ios/common/.*$",
      "android/common/.*$",
      "csharp/common/.*$",
      "http/common/.*$",
      "raspberrypi/common/.*$",
      "cpp/common/.*$",
      "javascript/common/.*$",
      "rust/common/.*$"
    ],
    "selectors": {
      "lvl0": ".theme-doc-markdown header h1",
      "lvl1": ".theme-doc-markdown h2",
      "lvl2": ".theme-doc-markdown h3",
      "lvl3": ".theme-doc-markdown h4",
      "text": ".theme-doc-markdown p,.theme-doc-markdown ol"
    },
    "min_indexed_level": 1
  }